<prism:PrismApplication x:Class="EcgEngine.Editor.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:EcgEngine.Editor.WPF"
             xmlns:converter="clr-namespace:EcgEngine.Core.Converters;assembly=EcgEngine.Core"
             xmlns:prism="http://prismlibrary.com/">
    <Application.Resources>
        <ResourceDictionary>
            <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
            <converter:StringToFloatConverter x:Key="StringToFloatConverter" />
            <converter:IntegerToStringConverter x:Key="IntegerToStringConverter" />
        </ResourceDictionary>
    </Application.Resources>
</prism:PrismApplication>
