                    xmlns:local="clr-namespace:EcgEngine.Editor.WPF.Styles">

    <Style x:Key="Icon16px" TargetType="Image">
        <Setter Property="Width" Value="16" />
        <Setter Property="Height" Value="16" />
    </Style>

    <Style x:Key="Icon20px" TargetType="Image">
